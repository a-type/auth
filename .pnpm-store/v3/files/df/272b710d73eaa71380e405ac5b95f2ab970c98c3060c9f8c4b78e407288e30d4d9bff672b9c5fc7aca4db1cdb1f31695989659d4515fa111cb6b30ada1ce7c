import React from 'react';
import { WeekDay } from './constants';
declare const _default: {
    title: string;
    component: React.ForwardRefExoticComponent<Omit<React.HTMLAttributes<HTMLDivElement>, "value" | "onChange"> & {
        onDisplayChange?: ((newValue: {
            month: number;
            year: number;
        }) => any) | undefined;
        displayYear: number;
        displayMonth: number;
        value?: Date | null | undefined;
        onChange?: ((value: Date | null) => any) | undefined;
        rangeValue?: {
            start: Date | null;
            end: Date | null;
        } | undefined;
        onRangeChange?: ((range: {
            start: Date | null;
            end: Date | null;
        }) => any) | undefined;
        onRangeStartChange?: ((range: Date) => any) | undefined;
        getDateEnabled?: ((date: Date) => boolean) | undefined;
        defaultDate?: Date | undefined;
        weekStartsOn?: WeekDay | undefined;
    } & React.RefAttributes<any>>;
};
export default _default;
export declare const simple: () => JSX.Element;
export declare const range: () => JSX.Element;
export declare const dayLabels: () => JSX.Element;
export declare const weekStartsOnMonday: () => JSX.Element;
export declare const wideRange: () => JSX.Element;
export declare const disabledDays: () => JSX.Element;
export declare const stylized: () => JSX.Element;
