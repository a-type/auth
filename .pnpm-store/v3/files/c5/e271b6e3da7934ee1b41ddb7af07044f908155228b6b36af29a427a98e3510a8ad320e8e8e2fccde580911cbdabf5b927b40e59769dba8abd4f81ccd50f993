import type { StoryObj } from '@storybook/react';
import { Tooltip } from './Tooltip.js';
declare const meta: {
    title: string;
    component: typeof Tooltip;
    argTypes: {};
    parameters: {
        controls: {
            expanded: boolean;
        };
    };
    args: {
        content: string;
    };
};
export default meta;
type Story = StoryObj<typeof Tooltip>;
export declare const Default: Story;
