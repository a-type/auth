// @unocss-include
import { jsx as _jsx } from "react/jsx-runtime";
import { Tooltip } from './Tooltip.js';
const meta = {
    title: 'Tooltip',
    component: Tooltip,
    argTypes: {},
    parameters: {
        controls: { expanded: true },
    },
    args: {
        content: 'hello world',
    },
};
export default meta;
export const Default = {
    render: (args) => (_jsx(Tooltip, Object.assign({}, args, { children: _jsx("button", { children: "Hover me" }) }))),
};
